{
  "name": "sunny-payments",
  "version": "1.0.0",
  "description": "Sunny Payment Gateway - A comprehensive global payment processing solution",
  "main": "src/index.js",
  "bin": {
    "sunny": "./src/cli/index.js"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:security": "jest --config jest.security.config.js",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.{js,jsx}\" \"tests/**/*.{js,jsx}\"",
    "eject": "react-scripts eject",
    "build:sdk": "rollup -c rollup.config.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:migrate:mongo": "node src/migrations/migrationRunner.js",
    "db:migrate:mongo:down": "node src/migrations/migrationRunner.js down",
    "db:migrate:status": "node src/migrations/migrationRunner.js status",
    "start:server": "node server.js",
    "dev:server": "nodemon server.js",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start\"",
    "cli": "node src/cli/index.js",
    "start:helios": "node scripts/start-helios.js",
    "dev:marketing": "pnpm --filter @sunny/marketing run dev",
    "dev:institutions": "pnpm --filter @sunny/institutions-portal run dev",
    "dev:business": "pnpm --filter @sunny/business-dashboard run dev",
    "dev:user": "pnpm --filter @sunny/user-dashboard run dev",
    "dev:admin": "pnpm --filter @sunny/admin-dashboard run dev",
    "dev:developer": "pnpm --filter @sunny/developer-portal run dev",
    "build:marketing": "pnpm --filter @sunny/marketing run build",
    "build:institutions": "pnpm --filter @sunny/institutions-portal run build",
    "build:business": "pnpm --filter @sunny/business-dashboard run build",
    "build:user": "pnpm --filter @sunny/user-dashboard run build",
    "build:admin": "pnpm --filter @sunny/admin-dashboard run build",
    "build:developer": "pnpm --filter @sunny/developer-portal run build",
    "build:packages": "pnpm -r --filter='@sunny/*' run build"
  },
